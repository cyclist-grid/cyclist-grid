{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "gulp serve",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "gulp-serve"],
      "console": "integratedTerminal",
      "serverReadyAction": {
        "pattern": "https?://localhost:([0-9]+)",
        "uriFormat": "http://localhost:%s/examples/typography.html"
      }
    }
  ]
}